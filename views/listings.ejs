<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="Yoga Studio Template">
    <meta name="keywords" content="Yoga, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>인절미BS | 관광지</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900&display=swap" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="css/flaticon.css" type="text/css">
    <link rel="stylesheet" href="css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<%if(loginfirst){%>
<script>alert("로그인이 필요한 서비스입니다.")</script>
<%}%>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header-section listings">
        <div class="container-fluid">
            <div class="logo">
                <a href="./index.ejs?"><img src="img/logo.png" alt=""></a>
            </div>
            <nav class="main-menu mobile-menu">
                <ul>
                    <li>
                        <a href="index.ejs?">홈</a>
                    </li>
                    <li class="active">
                        <a href="listings.ejs?">관광지</a>
                    </li>
                    <li>
                        <a href="contact.ejs?">문의</a>
                    </li>
                </ul>
            </nav>
            <div class="header-right">
                <div class="user-access">
                        <%if(loggedin){%>
                            <li id="login_li"><a href="#" id="current"><%=user_id%> 님</a>
                                <ul>
                                    <li><a href="/views/Register/Modify.ejs">회원정보 수정</a></li>
                                    <li><a href="/views/logout">로그아웃</a></li>
                                </ul>
                            </li>
                        <%}else{%>
                            <a href="/views/Register/Login.ejs?" class="primary-btn">로그인</a>
                        <%}%>
                </div>
                <!-- <a href="#" class="primary-btn">Add Listing</a> -->
            </div>
            <div id="mobile-menu-wrap"></div>
        </div>
    </header>
    <!-- Header End -->

    <!-- Map Section Begin -->
    <div class="map">
        <iframe
            height="550" style="border:1;" allowfullscreen="" id="listing-map"></iframe>
    </div>
    <!-- Map Section End -->

    <!-- Search Filter Section Begin -->
    <section class="search-filter spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <form action="/views/listings.ejs" class="filter-search">
                        <div class="category-search">
                            <h5>카테고리</h5>
                            <select name="category" class="ca-search" id="AttractionCategory2">
                                <option>전체</option>
                                <option>자연</option>
                                <option>문화관광</option>
                                <option>테마관광지</option>
                                <option>도보</option>
                                <option>레저/체험</option>
                            </select>
                        </div>
                        <div class="location-search">
                            <h5>위치</h5>
                            <select name="location" class="lo-search" id="AttractionLocation2">
                                <option>전체</option>
                                <option>서울</option>
                                <option>부산</option>
                                <option>대구</option>
                                <option>인천</option>
                                <option>광주</option>
                                <option>대전</option>
                                <option>울산</option>
                                <option>세종</option>
                                <option>경기</option>
                                <option>강원</option>
                                <option>충북</option>
                                <option>충남</option>
                                <option>전북</option>
                                <option>전남</option>
                                <option>경북</option>
                                <option>경남</option>
                                <option>제주</option>
                            </select>
                        </div>
                        <button type="submit">검색</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
    <!-- Search Filter Section End -->

    <!-- Filter Section Begin -->
    <section class="filter-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="filter-left">
                        <div class="rating-filter">
                            <h3>평점</h3>
                            <div class="rating-option">
                                <div class="ro-item" id="five">
                                    <input type="checkbox">
                                    <label>5.0</label>
                                    <div class="rating-pic">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                    </div>
                                </div>
                                <div class="ro-item" id="four">
                                    <input type="checkbox">
                                    <label>4.0</label>
                                    <div class="rating-pic">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                    </div>
                                </div>
                                <div class="ro-item" id="three">
                                    <input type="checkbox">
                                    <label>3.0</label>
                                    <div class="rating-pic">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                    </div>
                                </div>
                                <div class="ro-item" id="two">
                                    <input type="checkbox">
                                    <label>2.0</label>
                                    <div class="rating-pic">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                    </div>
                                </div>
                                <div class="ro-item" id="one">
                                    <input type="checkbox">
                                    <label>1.0</label>
                                    <div class="rating-pic">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                        <i class="fa fa-star grey-bg"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a href="./register.ejs?" class="primary-btn">등록</a>
                    </div>
                </div>
                <div class="col-lg-9" id="viewtable">
                    <div class="row">
                        <div class="col-lg-12">
                            <form action="#" class="arrange-select">
                                <span>정렬</span>
                                <select>
                                    <option>최신순</option>
                                    <option>오래된순</option>
                                </select>
                            </form>
                        </div>
                        <% for (var index in result){ if (index >8) break;%>
                        <div class="col-lg-4 col-sm-6">
                            <a class="arrange-items" href=<%="single-listing.ejs?id="+result[index].id%>>
                                <div class="arrange-pic">
                                    <img src="<%="img/uploaded/"+result[index].picture%>" onerror="this.src='img/arrange/arrange-9.jpg';"><!--!-->
                                    <div class="rating"><%= result[index].score%></div>
                                </div>
                                <div class="arrange-text">
                                    <h5> <%= result[index].title%> </h5>
                                    <span><%= result[index].address%></span>
                                    <p>요금 : <%= (result[index].fee ==0)?'무료': result[index].fee%></p>
                                    <div class="open"><%= result[index].opentime%></div>
                                </div>
                            </a>
                        </div>
                        <%}%>
                            <!-- 아래 표 부분이 db에서 붙여버린것. 이런형식으로 db에서 가져오면됨. -->
                    </div>

                    <div class="col-lg-12 text-right">
                        <div class="pagination-num">
                            <%for(var i = 0; i<result.length/9 +1;i++) {%>
                            <% var targetpage = _url%>
                            <%-'<a href='+_url+'>'%><%='i'%></a>
                            <%}%>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </section>
    <!-- Filter Section End -->

    <!-- Footer Section Begin -->
    <footer class="footer-section spad">
        <div class="container">
        </div>
    </footer>
    <!-- Footer Section End -->

    <!-- Js Plugins -->
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/jquery.slicknav.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/jquery.nice-select.min.js"></script>
    <script src="js/mixitup.min.js"></script>
    <script src="js/main.js"></script>
</body>

</html>